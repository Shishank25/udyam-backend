generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model udyamFormSubmissions {
  id             Int       @id @default(autoincrement())
  aadhaar_number String    @db.VarChar(12)
  aadhaar_name   String    @db.VarChar(100)
  pan_number     String    @db.VarChar(10)
  pan_name       String    @db.VarChar(100)
  dob_pan        DateTime? @db.Date
  org_type       String    @db.VarChar(50)
  created_at     DateTime? @default(now()) @db.Timestamp(6)
}
